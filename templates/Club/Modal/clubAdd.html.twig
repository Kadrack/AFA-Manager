<div class="modal-dialog modal-dialog-centered" role="document">
    <div class="modal-content">
        {{ form_start(form) }}
        <div class="modal-header">
            <h5 class="modal-title">Ajouter un dojo</h5>
        </div>
        <div class="modal-body">
            {{ form_row(form.ClubId) }}
            {{ form_row(form.ClubName) }}
            {{ form_row(form.ClubAddress) }}
            {{ form_row(form.ClubZip) }}
            {{ form_row(form.ClubCity) }}
            {{ form_row(form.ClubProvince) }}
            {{ form_row(form.ClubCreation) }}
            {{ form_row(form.ClubType) }}
        </div>
        <div class="modal-footer">
            {{ form_widget(form.Submit) }}
            <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Annuler</button>
        </div>
        {{ form_end(form) }}
    </div>
</div>
